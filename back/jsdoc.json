{
    "plugins": [
        "plugins/markdown",
        "node_modules/better-docs/typescript"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            "src"
        ],
        "exclude": [
            "dist",
            "docs"
        ],
        "includePattern": "\\.(ts|tsx)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "search": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "template": "node_modules/better-docs"
    }
}